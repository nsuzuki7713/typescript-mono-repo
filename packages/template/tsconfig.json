{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "sourceMap": true,
  },
  "include": ["src/**/*"]
}
